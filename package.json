{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "dependencies": {
    "@remix-run/react": "^1.2.1",
    "@remix-run/vercel": "^1.2.1",
    "front-matter": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.1",
    "@remix-run/eslint-config": "^1.2.2",
    "@remix-run/serve": "^1.2.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "cross-env": "^7.0.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "scripts": {
    "postinstall": "remix setup node",
    "build": "cross-env NODE_ENV=production remix build",
    "dev": "cross-env NODE_ENV=development remix dev",
    "type-check": "tsc -p tsconfig.json",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "eslint --ext .tsx,.ts .",
    "lint:fix": "eslint --ext .tsx,.ts . --fix",
    "prepare": "husky install && cp -a .husky/. .git/hooks/"
  },
  "lint-staged": {
    "./app/**/*.{ts,tsx}": [
      "npm run lint"
    ],
    "**/*": [
      "npm run prettier"
    ]
  }
}
