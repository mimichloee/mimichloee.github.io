{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "dependencies": {
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@remix-run/vercel": "^1.1.3",
    "esbuild": "^0.13.4",
    "front-matter": "^4.0.2",
    "marked": "^4.0.12",
    "mdx-bundler": "^8.0.1",
    "postcss-import": "^14.0.2",
    "prism-themes": "^1.9.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reading-time": "^1.5.0",
    "rehype-highlight": "^5.0.2",
    "remark-prism": "^1.3.6",
    "remark-slug": "^7.0.1",
    "remix": "^1.2.1",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@remix-run/eslint-config": "^1.2.2",
    "@tailwindcss/typography": "^0.5.2",
    "@types/marked": "^4.0.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/remark-prism": "^1.3.0",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "husky": "^7.0.4",
    "postcss": "^8.4.7",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "scripts": {
    "postinstall": "remix setup node",
    "build:css": "postcss styles/**/*.css --base styles --dir app/styles",
    "build": "cross-env NODE_ENV=production npm run build:css && remix build",
    "dev:css": "npm run build:css -- --w",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run dev:css\" \"remix dev\"",
    "type-check": "tsc -p tsconfig.json",
    "prettier": "prettier --check \"**/*.{ts,tsx,mdx}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,tsx,mdx}\"",
    "lint": "eslint \"./app/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"./app/**/*.{ts,tsx}\"",
    "prepare": "husky install && cp -a .husky/. .git/hooks/"
  }
}
