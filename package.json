{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "postinstall": "remix setup node",
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "npx tailwindcss -o ./app/styles/tailwind.css",
    "type-check": "tsc -p tsconfig.json",
    "prettier": "prettier --check \"**/*.{ts,tsx,mdx}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,tsx,mdx}\"",
    "lint": "eslint \"./app/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"./app/**/*.{ts,tsx}\"",
    "prepare": "husky install && cp -a .husky/. .git/hooks/"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.11.0",
    "@remix-run/react": "^1.2.3",
    "@remix-run/vercel": "^1.2.3",
    "esbuild": "^0.12.9",
    "front-matter": "^4.0.2",
    "graphql": "16.3.0",
    "graphql-request": "4.1.0",
    "marked": "^4.0.12",
    "mdx-bundler": "^8.0.1",
    "postcss-import": "^14.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rehype-highlight": "^5.0.2",
    "remark-prism": "^1.3.6",
    "remark-slug": "^7.0.1",
    "remix": "^1.2.1",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "^1.2.3",
    "@remix-run/serve": "^1.2.3",
    "@tailwindcss/typography": "^0.5.2",
    "@types/marked": "^4.0.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/remark-prism": "^1.3.0",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "esbuild-register": "^3.3.2",
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.7",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "prisma": "^3.11.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
